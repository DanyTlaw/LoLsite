<div class="col-md-12">
  <% if @matchup.errors.any? %>
    <h2>Errors</h2>
    <ul>
    <% @matchup.errors.each do |attr, message|%>
      <li><%=message%></li>
    <% end %>
    </ul>
  <% end %>
    <%= form_for(@matchup) do |f| %>
      <div class="panel panel-warning">
        <div class="panel-heading">Create your Matchup</div>
          <div class="panel-body">
            <%= f.hidden_field :champ_eins, id: "champLinks" %>
            <%= f.hidden_field :champ_zwei, id: "champRechts" %>
            <!--Top Section for choose : Titel, Lane, Champ1 und Champ 2-->
            <div class="panel panel-default">
                 <div class="panel-body">
                  <div class="matchup-info", style ="width: 60%;margin-left:20%;margin-right:20%">
                  <div class="col-md-3">
                      <div class="champAdd" id="left" data-toggle="modal" data-target="#modal_champ">
                      <% if @matchup.champ_eins != "" %>
                        <img src="<%= urlImgChamps(@matchup.champ_eins) %>" id="" name="<%= @matchup.champ_eins%>" style="max-width: 100%;max-height: 100%;">
                      <% else %>
                        <img src="" id="" name="" style="">
                      <% end %>
                      </div>
                  </div>
                  
                  <div class="col-md-6">
                      <div class="vs">
                      VS
                      </div>
                  </div>
                  <div class="col-md-3">
                      <div class="champAdd" id="right" data-toggle="modal" data-target="#modal_champ">
                      <% if @matchup.champ_zwei != "" %>                    
                        <img src="<%= urlImgChamps(@matchup.champ_zwei) %>" id="" name="<%= @matchup.champ_zwei%>" style="max-width: 100%;max-height: 100%;">
                      <% else %>
                        <img src="" id="" name="" style="">
                      <% end %>
                      </div>
                  </div>                  
                  </div>               
              </div>
            </div>

            <%= f.hidden_field :runes, id: "runesHidden"%>
            <!-- Rune Section-->
            <div class="col-md-12">
              <div class="panel panel-default">
                <div class="panel-heading">Runes</div>
                <div class="panel-body">
                  <div class="col-md-6">
                    <div class="panel panel-default">
                      <div class="panel-heading">Available Runes</div>
                      <div class="panel-body">
                        <!-- Button runes auswÃ¤hlen section-->
                        <div class="runes-buttons">
                          <div class="btn btn-default runeBtn" type="red">Marks</div>
                          <div class="btn btn-default runeBtn" type="yellow">Seals</div>
                          <div class="btn btn-default runeBtn" type="blue">Glyphs</div>
                          <div class="btn btn-default runeBtn" type="black">Quintessences</div>
                        </div>
                        <!-- Rune list section-->
                        <div class="runes">
                        <% rcounter = 0 %>
                          <% $runes.each do |rune| %>                      
                            <% if $runeTier[rcounter] == "3" %>
                            <% url = $apiurl + "/rune/" + $imgRune[rcounter]%>
                              <div class="well <%= $runeType[rcounter]%>" id="rune" side="left" type="<%= $runeType[rcounter]%>" >
                              <!-- Setzt die Anzahl der Runen auf der linken seite-->
                                <div class="rune-number">
                                <% if !@matchup.runes.blank? %>
                                <% hit = false%>
                                  <% arrRunes = runeStringSplit(@matchup.runes) %>

                                  <% arrRunes.each do |arr|%>
                                  <% runeBeforeSpl = Array.new%>
                                  <% runeBeforeSpl = arr.split(",")%>
                                  <% runeBeforeSpl.each do |splitarr| %>
                                  <% runeSpl = Array.new %>
                                    <% runeSpl = splitarr.split("x")%>
                                    <% if runeSpl[1] == rune.name %>
                                      
                                      <% if $runeType[rcounter] == "black" %>
                                        <%= 3-runeSpl[0].to_i %>
                                      <% else %>
                                        <%= 9-runeSpl[0].to_i %>
                                      <% end %>
                                      <% hit = true %>
                                    <% end %>
                                    <% end %>
                                  <% end %>
                                  <% if !hit %>
                                    <%if $runeType[rcounter] == "black" %>
                                        3
                                    <% else %>
                                        9
                                    <% end %>
                                  <% end %>
                                <% else %>
                                  <% if $runeType[rcounter] == "black"%>
                                    3
                                  <% else %>
                                    9
                                  <% end %>
                                <% end %>
                                </div> 
                                <div class="rune-img">
                                  <img src="<%= url %>" style="width: 40px; height:40px">
                                </div>  
                                <div class="rune-info">
                                  <div class="rune-name"><%= rune.name%></div>
                                  <div class="rune-stats"><%= rune.description%></div>    
                                </div>
                              </div>
                              <% end %>
                            <% rcounter += 1 %>
                          <% end %>
                        </div>
                      </div>
                    </div>
                  </div>
                  <!-- Selected Runes section -->
                  <div class="col-md-6">
                    <div class="panel panel-default">
                      <div class="panel-heading">Selected Runes</div>
                      <div class class="panel-body">
                        <div class="runes-selected">
                          <div class="selected-runes">                     
                            <p>Total Runes: <span id="rune" type="red"></span> Marks, <span id="rune" type="yellow"></span> Seals, <span id="rune" type="blue"></span> Glyphs, <span id="rune" type="black"></span> Quints Added</p>   
                          </div>
                          <div class="runeAdd">
                          <% if !@matchup.runes.blank? %>
                            <% rcounter = 0 %>
                            <% $runes.each do |rune| %>                 
                              <% if $runeTier[rcounter] == "3" %>
                                <% url = $apiurl + "/rune/" + $imgRune[rcounter]%>
                                <% arrRunes = runeStringSplit(@matchup.runes) %>

                                <% arrRunes.each do |arr|%>
                                  <% runeBeforeSpl = arr.split(",")%>
                                  <% runeBeforeSpl.each do |splitarr| %>
                                  <% runeSpl = Array.new %>
                                    <% runeSpl = splitarr.split("x")%>
                                  <% if runeSpl[1] == rune.name %> 
                                    <div class="well <%= $runeType[rcounter]%>" id="rune" side="left" type="<%= $runeType[rcounter]%>" >
                                      <div class="rune-number">
                                        <%= runeSpl[0].to_i %>                                              
                                      </div>                    
                                      <div class="rune-img">
                                         <img src="<%= url %>" style="width: 40px; height:40px">
                                      </div>  
                                      <div class="rune-info">
                                        <div class="rune-name"><%= rune.name%></div>
                                        <div class="rune-stats"><%= rune.description%></div>    
                                      </div>
                                    </div> 
                                    <% end %>  
                                  <% end %>
                                  <% end %> 
                                <% end %>
                              <% rcounter += 1%>
                            <% end %>  
                          <% end %>      
                          </div>
                        </div>
                      </div>                      
                    </div>
                  </div> 
                </div>
                  </div>
                </div>
              
