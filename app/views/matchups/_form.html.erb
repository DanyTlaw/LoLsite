<div class="modal fade" id="modal_item" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
      <div class="modal-content">
        <div class="modal-header">
          <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
          <h4 class="modal-title" id="myModalLabel">Choose your item</h4>
        </div>
        <div class="modal-body">
          <% counter = 0 %>
          <% $imgItem.each do |item| %>
            <img class="imgItem" src="<%= $apiurl + 'item/' + item%>" data-toggle='tooltip' data-html="true" data-placement='bottom' title='<%= $itemDesc[counter]%>' >
            <% counter+=1%>
          <% end %>
        </div>

        <div class="modal-footer">

        </div>
      </div>
    </div>
  </div>

</div>

